{
  "name": "@frfla/react-hooks",
  "version": "0.10.2",
  "description": "React Util Hooks",
  "repository": "https://github.com/frfla/react-hooks",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "keywords": [
    "react",
    "custom hooks",
    "hooks",
    "utility",
    "util",
    "modal",
    "form",
    "clipboard"
  ],
  "scripts": {
    "test-build": "pnpx jest --verbose",
    "test": "pnpx jest --watchAll --verbose",
    "coverage": "pnpx jest --coverage",
    "build": "pnpm build-package .",
    "build-package": "bunchee --cwd",
    "release": "npm publish"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./dist/es/index.d.mts",
        "default": "./dist/es/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "./dist/**/*.{js,d.ts,mjs,d.mts}",
    "./README.md",
    "./README.*.md",
    "./package.json"
  ],
  "author": "vvyre <seung.yoon.yu@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@frfla/laser": "^0.1.0",
    "@swc/cli": "^0.3.12",
    "@swc/core": "^1.4.12",
    "@swc/jest": "^0.2.36",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^14.2.2",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "29.5.12",
    "@types/node": "^20.12.4",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.3.0",
    "axios": "^1.6.8",
    "bunchee": "^5.1.2",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-jest-dom": "^5.2.0",
    "eslint-plugin-react": "7.34.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "eslint-plugin-testing-library": "6.2.0",
    "jest": "29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "nock": "^13.5.4",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "peerDependencies": {
    "react": ">=18"
  },
  "prettier": {
    "tabWidth": 2,
    "printWidth": 100,
    "semi": false,
    "useTabs": false,
    "singleQuote": true,
    "arrowParens": "avoid",
    "trailingComma": "none"
  },
